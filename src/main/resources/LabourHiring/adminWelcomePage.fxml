<AnchorPane opacity="0.96" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LabourHiring.adminWelcomePageController">
   <children>
      <Pane fx:id="pane" prefHeight="600.0" prefWidth="218.0" style="-fx-background-color: Lavender;">
         <children>
            <Button fx:id="LOgoutButton" alignment="CENTER" layoutX="50.0" layoutY="520.0" mnemonicParsing="false" onAction="#OnActionLogoutButton" prefHeight="35.0" prefWidth="99.0" style="-fx-background-color: SteelBlue;" text="Log Out" />
            <Circle fx:id="adminprofile" fill="#d7e3ee" layoutX="100.0" layoutY="48.0" radius="30.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.1" />
            <ImageView fx:id="adminProfileImage" fitHeight="43.0" fitWidth="42.0" layoutX="79.0" layoutY="27.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Project Image/signupUserLogo.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <Button fx:id="adminProfileButton" layoutX="52.0" layoutY="97.0" mnemonicParsing="false" onAction="#onActionAdminProfileButton" prefHeight="35.0" prefWidth="101.0" style="-fx-background-color: SteelBlue;" text="Admin Profile" />
