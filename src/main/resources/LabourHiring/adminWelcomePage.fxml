      <ImageView fx:id="databaseLabelImage" fitHeight="77.0" fitWidth="99.0" layoutX="421.0" layoutY="262.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Project Image/database.png" />
         </image>
      </ImageView>
      <Label fx:id="manageDatabaseLabel" layoutX="399.0" layoutY="339.0" prefHeight="19.0" prefWidth="121.0" text="Manage Database">
         <font>
            <Font name="AkayaTelivigala-Regular" size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
